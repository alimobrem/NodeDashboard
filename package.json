{
  "name": "node-dashboard",
  "version": "0.0.1",
  "description": "OpenShift Console Plugin for Node Dashboard - comprehensive cluster node monitoring and management",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alimobrem/NodeDashboard.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode=production",
    "start": "NODE_ENV=development webpack serve --mode=development",
    "start-console": "yarn run start-console:standalone",
    "start-console:standalone": "./start-console.sh",
    "i18n": "i18next-parser",
    "ts-node": "ts-node",
    "lint": "eslint --ext .tsx,.ts src/ --no-error-on-unmatched-pattern",
    "lint:fix": "yarn run lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/",
    "test:integration": "jest --testPathPattern=integration-tests/",
    "cypress:open": "cypress open --config-file integration-tests/cypress.config.js",
    "cypress:run": "cypress run --config-file integration-tests/cypress.config.js",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@openshift-console/dynamic-plugin-sdk": "1.4.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "1.3.0",
    "@patternfly/react-core": "^6.2.2",
    "@patternfly/react-icons": "^6.2.2",
    "@patternfly/react-table": "^6.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "7.1.2",
    "cypress": "^14.5.1",
    "cypress-multi-reporters": "^1.6.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.1",
    "i18next-parser": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-router": "5.3.x",
    "react-router-dom": "5.3.x",
    "style-loader": "^3.3.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^34.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "5.75.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^5.2.2"
  },
  "consolePlugin": {
    "name": "node-dashboard",
    "version": "0.0.1",
    "displayName": "Nodes Dashboard",
    "description": "OpenShift Console plugin for comprehensive cluster node monitoring and management with real-time updates.",
    "exposedModules": {
      "NodesDashboard": "./components/NodesDashboard"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "peerDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "i18next": "^23.11.5",
    "mocha": "^10.5.1"
  },
  "dependencies": {
    "esbuild": "^0.25.5"
  }
}
